<div id="videos-container" class="contents-container">

<div class="video-card">
<div class="video-title">
  <h1>{{videos.title}}</h1>
 </div>
  <iframe src="{{videos.videoUrl}}" frameborder="0" class="video-player"></iframe>
 <div class="video-description">
     <p>{{videos.description}}</p>
 </div> 
 <a id="edit" href="/videos/{{videos._id}}/edit">Edit</a>
 <form action="/videos/{{videos._id}}/deletions" method="post">
 <button id="delete" type="submit">Delete</button>
 </form>
</div>
    </div>